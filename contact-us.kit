<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.css">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@500;600&display=swap" rel="stylesheet">
    <title>Document</title>
</head>
<body>
    <div class="wrapper">
        <!--@import "navbar.html"-->
        
        <!-- Main content -->
        <div class="contact-us">
        
            <!-- Main top part -->
            <div class="contact-us__top-conteinet">
                <!-- Get in tuch -->
                <div class="contact-us__get-in-tuch">
                    <h1 class="contact-us__title">Have a question ? Letâ€™s get in touch with us.</h1>
                    <p class="contact-us__text">Fill up the Form and ou team will get back to within 24 hrs</p>
                </div>
                <!-- Contact us -->
                <div class="contact-us__info">
                    <!-- Location -->
                    <div class="contact-us__location">
                        <h2 class="contact-us__subtitle">Location</h2>
                        <p class="contact-us__text">DLF Cybercity, Bhubaneswar, <br> India, &52050 </p>
                    </div>
                    <!-- Contacts -->
                    <div>
                        <h2 class="contact-us__subtitle">Contact Us</h2>
                        <p> <a href="tel:+3802079932905"> <span class="contact-us__text"> 020 7993 2905</span></a>
                            <br> <a href="mailto:hi@finsweet.com"> <span class="contact-us__text">hi@finsweet.com</span> </a>
                        </p>
                    </div>
                </div>
            </div>
            <!-- Main bottom part -->
            <div class="contact-us__bottom-conteiner">
                <!-- Form -->
                <form action="#" method="get">
                    <div class="contact-us__form">
                        <input placeholder="First Name" tabindex="1" type="text" name="first-name" value="">
                        <input placeholder="Last Name" tabindex="2" type="text" name="last-name" value="">
                        <input placeholder="Email address" tabindex="3" type="email" name="mail" id="">
                        <textarea class="contact-us__text-area" placeholder="Type message" tabindex="4" name="coment" cols="50"
                            rows="7"></textarea>
        
                    </div>
        
                    <input class="contact-us__button" type="button" value="Submit" name="submit">
        
        
                </form>
        
                <!-- Map -->
                <div>
                    <img src="img/map.png" alt="Map">
                </div>
            </div>
        </div>


        <!--@import "footer.html"-->
    </div>

</body>
</html>

